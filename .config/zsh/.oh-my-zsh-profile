# oh-my-zsh profile

# path to oh-my-zsh installation
export ZSH="$ZDOTDIR/.oh-my-zsh"

# if oh-my-zsh is snot installed return
if [ ! -d "$ZSH" ]; then
    return
fi

# set z plugin path
export _Z_DATA="$ZCACHEDIR/.z"


# theme
ZSH_THEME="agnoster"

# plugins
plugins=(
  git
  z
  bundler
  dotenv
  rake
  zsh-syntax-highlighting
  zsh-autosuggestions
)

source "$ZSH/oh-my-zsh.sh"
